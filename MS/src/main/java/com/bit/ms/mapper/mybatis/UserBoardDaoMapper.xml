<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bit.ms.member.dao.UserBoardDaoInterface">

	<select id="UserBoardSelectList" resultType="UserBoardVO">

		select * from
		userBoard order
		by uboard_id desc limit #{firstRow}, 10;

	</select>

	<select id="UserBoardPageCount" resultType="int">

		select count(*) from
		userBoard;

	</select>

	<insert id="insertUserBoard" parameterType="UserBoardVO"
		useGeneratedKeys="true" keyProperty="uboard_id">

		insert into
		userBoard(uboard_title, uboard_con)
		values(#{uboard_title}, #{uboard_con})

	</insert>


</mapper>